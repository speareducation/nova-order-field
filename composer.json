{
    "name": "speareducation/nova-order-field",
    "description": "A Laravel Nova field.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "laravel/nova": ">=2.0",
        "spatie/eloquent-sortable": "^3.4|^4.0"
    },
    "autoload": {
        "psr-4": {
            "Spear\\NovaOrderField\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spear\\NovaOrderField\\OrderFieldServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "laravel/nova-devtool": "^1.8"
    }
}
